{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "refal",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "module",
          "content": {
            "type": "SYMBOL",
            "name": "module"
          }
        },
        {
          "type": "FIELD",
          "name": "function",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "function"
            }
          }
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$MODULE"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "id"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "id"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "sentence",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "sentence"
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "sentence": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "pattern",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_pattern_obj"
            }
          }
        },
        {
          "type": "FIELD",
          "name": "conditions",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "condition"
            }
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "rewrite",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_obj"
            }
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "condition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "FIELD",
          "name": "test",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_obj"
            }
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "pattern",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_pattern_obj"
            }
          }
        }
      ]
    },
    "_pattern_obj": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "e_var"
        },
        {
          "type": "SYMBOL",
          "name": "s_var"
        },
        {
          "type": "SYMBOL",
          "name": "t_var"
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "_pattern_obj_br"
        }
      ]
    },
    "_pattern_obj_br": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "str_br_l"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_pattern_obj"
          }
        },
        {
          "type": "SYMBOL",
          "name": "str_br_r"
        }
      ]
    },
    "_obj": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "e_var"
        },
        {
          "type": "SYMBOL",
          "name": "s_var"
        },
        {
          "type": "SYMBOL",
          "name": "t_var"
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "_obj_br"
        },
        {
          "type": "SYMBOL",
          "name": "_obj_fn"
        }
      ]
    },
    "_obj_br": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "str_br_l"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_obj"
          }
        },
        {
          "type": "SYMBOL",
          "name": "str_br_r"
        }
      ]
    },
    "_obj_fn": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "fun_br_l"
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_obj"
          }
        },
        {
          "type": "SYMBOL",
          "name": "fun_br_r"
        }
      ]
    },
    "_symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "q_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        }
      ]
    },
    "str_br_l": {
      "type": "STRING",
      "value": "("
    },
    "str_br_r": {
      "type": "STRING",
      "value": ")"
    },
    "fun_br_l": {
      "type": "STRING",
      "value": "<"
    },
    "fun_br_r": {
      "type": "STRING",
      "value": ">"
    },
    "q_symbol": {
      "type": "PATTERN",
      "value": "'([^'\\t\\n\\f])*'"
    },
    "e_var": {
      "type": "PATTERN",
      "value": "\\$e\\.([a-zA-Z][-_a-zA-Z0-9]*|[0-9]+)"
    },
    "s_var": {
      "type": "PATTERN",
      "value": "\\$s\\.([a-zA-Z][-_a-zA-Z0-9]*|[0-9]+)"
    },
    "t_var": {
      "type": "PATTERN",
      "value": "\\$t\\.([a-zA-Z][-_a-zA-Z0-9]*|[0-9]+)"
    },
    "id": {
      "type": "PATTERN",
      "value": "[a-zA-Z][-_a-zA-Z0-9]*"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}